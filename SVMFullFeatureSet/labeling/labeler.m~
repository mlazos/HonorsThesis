test_set = dir('../ext/*.jpg');
load bandhdata
curr_data_len = length(image_data);
ext_len = length(test_set);
imnum = length(image_data);
new_image_data = repmat(struct('name','n','image',zeros(2000,2000),'ground_truth',zeros(2000,2000))...
    ,[ext_len + curr_data_len,1]);
new_image_data(1:curr_data_len) = image_data;
lowpass = fspecial('gaussian', [5,5], 1);
for i = 1:
    figure;
    im = imread(['../test_set_ext/',test_set(i).name]);
    im = imfilter(im,lowpass);
    im = im(1:4:end,1:4:end,:);
    mask = roipoly(im);
    imnum = imnum + 1;
    new_image_data(imnum).name = test_set(i).name;
    new_image_data(imnum).image = im;
    new_image_data(imnum).ground_truth = mask;
end